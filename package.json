{
  "name": "archiver",
  "version": "1.0.0",
  "description": "Lambda function to upload bookmark files to S3 and B2.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:ts": "tsup src/index.ts --env.NODE_ENV production --minify",
    "build:prod": "NODE_ENV=production webpack",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "eslint --fix \"src/*.ts\"",
    "publish": "NODE_ENV=production wrangler publish",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fourjuaneight/archiver.git"
  },
  "author": "Juan Villela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourjuaneight/archiver/issues"
  },
  "homepage": "https://github.com/fourjuaneight/archiver#readme",
  "dependencies": {
    "@mozilla/readability": "^0.4.1",
    "aws-sdk": "^2.875.0",
    "canvas": "^2.7.0",
    "dotenv": "^8.2.0",
    "jsdom": "^16.5.2",
    "node-fetch": "^2.6.1",
    "turndown": "^7.0.0",
    "uuid": "^3.3.2",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.1",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "@types/turndown": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsup": "^4.8.21",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "browser": {
    "crypto": false
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
