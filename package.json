  },
  "homepage": "https://github.com/fourjuaneight/archiver#readme",
  "dependencies": {
    "@mozilla/readability": "^0.4.1",
    "aws-sdk": "^2.875.0",
    "dotenv": "^8.2.0",
    "jsdom": "^16.5.2",
    "node-fetch": "^2.6.1",
    "turndown": "^7.0.0",
    "uuid": "^3.3.2",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
